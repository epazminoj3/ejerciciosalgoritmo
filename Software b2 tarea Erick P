Funcion Ejercicio11()
	//Suma de dos números: Escribe un programa que tome dos números como entrada y muestre su suma.
	//Bosquejo
	//Entrada:Datos:Numero1=0?(Leida),Numero2=0?(Leido),Resultado=0?(Calculo)
	//Proceso:Calculo:Resultado=Numero1+Numero2
	//Salida:Mostrar Respuesta: ResultadoE
	
	//Entrada
	Definir Numero1,Numero2,Resultado Como Entero
	Numero1=0;Numero2=0;Resultado=0
	Escribir "Ingrese numero 1"
	Leer Numero1
	Escribir "Ingrese numero 2"
	Leer Numero2
	//Proceso
	Resultado=Numero1+Numero2
	//Salida
	Escribir "El resultado de la suma es ", Resultado
FinFuncion


Funcion Ejercicio12()
	//Área de un triángulo: Pide al usuario que ingrese la base y la altura de un triángulo,
	//luego calcula y muestra su área.
	//Bosquejo:
	//Entrada:Datos:Base=0?(Leido),Altura=0?(Leido),Área=0?(Calculo)
	//Proceso:Calculo:Área=Base*Altura
	//Salida:Mostrar Respuesta: Área
	
	//Entrada
	Definir Base,Altura,Área Como Entero
	Base=0;Altura=0;Área=0
	Escribir "Escriba la base del triangulo"
	Leer Base
	Escribir "Escriba la altura del triangulo"
	Leer Altura
	//Proceso
	Área=Base*Altura
	//Salida
	Escribir "El Área del triangulo es ", Área
FinFuncion


Funcion Ejercicio13()
	//Número par o impar: Solicita al usuario que ingrese un número e indica si es par o impar
	//Bosquejo:
	//Entrada:Datos: Numero1=0?(Leido)
	//Proceso:Calculo:Si Numero1 mod 2 == 0 Escribir "Par" SiNo Escribir "Impar"
	//Salida:Mostrar Respuesta: Par o impar
	
	//Entrada
	Definir Numero1 como Entero
	Numero1=0
	Escribir "Escriba un numero"
	Leer Numero1
	//Proceso
	Si Numero1 mod 2==0  Entonces 
		Escribir "Par"
	SiNo 
		Escribir "Impar"
	FinSi
FinFuncion


Funcion Ejercicio14()
	//Calculadora simple: Crea una calculadora que realice operaciones básicas como suma, resta, 
	//multiplicación y división, según la elección del usuario.
	//Bosquejo
	//Entrada:Datos Numero1=0?(Leido), Numero2=0?(Leido) ,Operacion=" "(+,-,*,/)(Leido),Resultado=0?(Calculo)
	//Proceso: Calculo: Resultado=Numero1 Operacion(+,-,*,/) Numero2
	//Salida: Mostrar Respuesta: Resultado
	
	//Entrada
	Definir Numero1,Numero2,Resultado Como Entero
	Definir Operacion Como Caracter
	Numero1=0;Numero2=0;Resultado=0;Operacion=" "
	Escribir "Introduzca la operacion a realizar(+,-,*,/)"
	Leer Operacion
	Escribir "Introduzca Numero 1"
	Leer Numero1 
	Escribir "Introduzca Numero 2"
	Leer Numero2
	//Proceso
	Si Operacion="+" Entonces
		Escribir"Su Respuesta es ",Resultado=Numero1+Numero2
	SiNo 
		Si Operacion="-" Entonces
			Resultado=Numero1-Numero2
			Escribir"Su Respuesta es ",Resultado
		SiNo
			Si Operacion="*" Entonces
				Resultado=Numero1*Numero2
				Escribir"Su Respuesta es ",Resultado
			SiNo
				Si Operacion="/" Entonces
					Resultado=Numero1/Numero2
					Escribir"Su Respuesta es ",Resultado
				SiNo
					Escribir"Error Ingrese una operacion valida(+,-,*,/)"
					
				FinSi
			FinSi
		FinSi
	FinSi
FinFuncion

Funcion Ejercicio15()
    //Tabla de multiplicar: Pide al usuario un número y muestra su tabla de multiplicar del 1 al 10
	
	//Bosquejo:
	//Entrada:Num=0?(Leida) 
	//Proceso: Si Num=1 Escribir "1x1=1 1x2=2 1x3=3 1x4=4 1x5=5 1x6=6 1x7=7 1x8=8 1x9=9 1x10=10"
	//Sino Si Num=2 Escribir "2x1=2 2x2=4 2x3=6 2x4=8 2x5=10 2x6=12 2x7=14 2x8=16 2x9=18 2x10=20"
	//Sino Si Num=3 Escribir "3x1=3 3x2=6 3x3=9 3x4=12 3x5=15 3x6=18 3x7=21 3x8=24 3x9=27 3x10=30".........
	//Salida:Muestra la tabla de multiplicar correspondiente
	
	//Entrada
	Definir Num como Entero
	Num=0
	Escribir"Ingrese un numero del 1 al 10 para mostrar su tabla "
	Leer Num
	//Proceso
	Si Num=1 Entonces
		Escribir "1x1=1 1x2=2 1x3=3 1x4=4 1x5=5 1x6=6 1x7=7 1x8=8 1x9=9 1x10=10"
	SiNo
		Si Num=2 Entonces
			Escribir "2x1=2 2x2=4 2x3=6 2x4=8 2x5=10 2x6=12 2x7=14 2x8=16 2x9=18 2x10=20" 
		Sino 
			Si Num=3 Entonces
				Escribir "3x1=3 3x2=6 3x3=9 3x4=12 3x5=15 3x6=18 3x7=21 3x8=24 3x9=27 3x10=30"
			SiNo
				Si Num=4 Entonces
					Escribir "4x1=4 4x2=8 4x3=12 4x4=16 4x5=20 4x6=24 4x7=28 4x8=32 4x9=36 4x10=40"
				Sino 
					Si Num=5 Entonces
						Escribir"5x1=5 5x2=10 5x3=15 5x4=20 5x5=25 5x6=30 5x7=35 5x8=40 5x9=45 5x10=50"
					Sino 
						Si Num=6 Entonces
							Escribir "6x1=6 6x2=12 6x3=18 6x4=24 6x5=30 6x6=36 6x7=42 6x8=48 6x9=54 6x10=60"
						Sino 
							Si Num=7 Entonces
								Escribir "7x1=7 7x2=14 7x3=21 7x4=28 7x5=35 7x6=42 7x7=49 7x8=56 7x9=63 7x10=70"
							Sino 
								Si Num=8 Entonces
									Escribir "8x1=8 8x2=16 8x3=24 8x4=32 8x5=40 8x6=48 8x7=56 8x8=64 8x9=72 8x10=80"
								Sino 
									Si Num=9 Entonces
										Escribir "9x1=9 9x2=18 9x3=21 9x4=36 9x5=45 9x6=54 9x7=63 9x8=72 9x9=81 9x10=90"
									Sino 
										Si Num=10 Entonces
											Escribir "10x1=10 10x2=20 10x3=30 10x4=40 10x5=50 10x6=60 10x7=70 10x8=80 10x9=90 10x10=100"
										Sino
											Escribir "Error, Escriba un numero del 1 al 10"
										FinSi
									FinSi
								FinSi
							FinSi
						FinSi
						
					FinSi
				FinSi
			FinSi
		FinSi
	FinSi
	
FinFuncion


Funcion Ejercicio16()
	//Copiar palabra: Escribe un programa que lea dos palabras y concatena en otra variable las dos palabras
	//Entrada: Palabra1=" "?(Leido) Palabra2=" "?(Leido) Palabra3=" "?(Calculo)
	//Proceso: Palabra3=Palabra1+Palabra2
	//Salida: Mostrar la palabra concatenada
	
	//Entrada
	Definir Palabra1,Palabra2,Palabra2 como caracter
	Palabra1=" " ;Palabra2=" ";Palabra3=" "
	Escribir "Escriba Frase 1"
	Leer Palabra1
	Escribir "Escriba Frase 2"
	Leer Palabra2
	//Proceso
	Palabra3=Palabra1+" "+Palabra2
	//Salida
	Escribir "Su Frase concatenada es: ",Palabra 3
FinFuncion


Funcion Ejercicio17()
	//Mayor de tres números: Solicita tres números y determina cuál es el mayor de ellos.
	
	//Bosquejo
	//Entrada: Numero1=0?(Leido) Numero2=0?(Leido) Numero3=0?(Leido)
	//Proceso: Si Numero1>Numero2 y Numero1>Numero3 Entonces Numero1 es Mayor
	//Sino Si Numero2>Numero1 y numero2>Numero3 Entonces Numero2 es Mayor
	//Sino Si Numero3>Numero1 y Numero3>Numero2 Entonces Numero3 es Mayor
	//Sino Ninguno es Mayor
	//Salida=Mostrar Mayor
	
	//Entrada:
	Definir Num1,Num2,Num3 como Entero
	Num1=0;Num2=0;Num3=0
	Escribir "Ingrese Numero 1"
	Leer Num1
	Escribir "Ingrese Numero 2"
	Leer Num2
	Escribir "Ingrese Numero 3"
	Leer Num3
	//Proceso
	Si Num1>Num2 y Num1>Num3 Entonces
		Escribir "El numero mayor es ",Num1
	Sino 
		Si Num2>Num1 y Num2>Num3 Entonces
			Escribir "El numero mayor es ",Num2
		Sino
			Si Num3>Num2 y Num3>Num1 Entonces
				Escribir "El numero mayor es ",Num3
			SiNo
				Escribir "Ninguno es Mayor"
			FinSi	
		FinSi	
	FinSi
FinFuncion


Funcion Ejercicio18()
	//Edad mínima para votar: Pregunta la edad del usuario y verifica si es elegible para votar (18 años o más).

	//Bosquejo
	//Entrada: edad=0? (leer)
	//Proceso: si edad >= 18 "Esta apto para votar" si no "No esta apto para votar"
	//Salida: Muestra si esta apto o no para votar
	Definir edad como entero
	
	//Entrada
	edad=0
	Escribir "Introduzca su edad"
	Leer edad
	//Poceso
	si edad>=18 Entonces
		Escribir "Si esta apto para votar"
	Sino 
		Escribir "No esta apto para votar"
	FinSi
FinFuncion


Funcion Ejercicio19()
	//Calculadora de BMI: Crea un programa que calcule el índice de masa corporal (BMI) 
	//a partir del peso y la altura del usuario, y luego indique si está en una categoría de peso saludable.
	
	//Bosquejo
	//Entrada: Peso=0.0?(Leido) Altura=0.0?(Leido) BMI=0.0?(Calculo)
	//Proceso: BMI=Peso/Altura*Altura   Si BMI < 18.5 Su peso no es saludable
	//SiNo Si BMI >= 18.5 Y bmi < 24.9 Su peso Es Saludable
	//SiNo Si BMI >= 24.9 Y bmi < 29.9 Esta en la categoria de sobrepeso
	//SiNo Esta en la categoria de obesidad Cuide su salud
	//Salida: mostrar la categoria de peso
	
	//Entrada
	Definir Peso,Altura,BMI Como Real
	Peso=0.0;Altura=0.0;BMI=0.0
	Escribir "Introduzca su Peso en kilogramos"
	Leer Peso
	Escribir "Introduzca su Altura en Metros"
	Leer Altura
	//Proceso
	BMI=Peso/(Altura*Altura)
	Escribir "Su BMI es de ",BMI
	Si BMI < 18.5 Entonces
		Escribir "Su peso no es saludable"
	SiNo
		Si BMI >= 18.5 Y bmi < 24.9 Entonces 
			Escribir "Su peso Es Saludable"
		SiNo
			Si BMI >= 24.9 Y BMI < 29.9 Entonces
				Escribir "Esta en la categoria de sobrepeso"
			SiNo
				Escribir "Esta en la categoria de obesidad, Cuide su salud"
			FinSi	
		FinSi
	FinSi
FinFuncion


Funcion Ejercicio20()
	//Número positivo, negativo o cero: Pide al usuario que ingrese un número y 
	//muestra si es positivo, negativo o cero
	
	//Bosquejo
	//Entrada: Num=0?(Leido)
	//Proceso: Si Num > 0 el numero es positivo SiNo Si Num < 0 el numero es negativo SiNo Si Num==0 el numero es 0 
	//SiNo Escribir "Error,Ingrese un numero correcto"
	//Salida: Mostrar si es negativo,positivo o cero
	
	//Entrada
	Definir Num Como Entero
	Num=0
	Escribir "Ingrese un numero"
	Leer Num
	//Proceso
	Si Num > 0 Entonces
		Escribir "El numero es positivo"
	SiNo
		Si Num < 0 Entonces
			Escribir "El numero es negativo"
		SiNo
			Si Num = 0 Entonces
				Escribir "El numero es 0"
			SiNo 
				Escribir "Error, Ingrese un numero correcto"
			FinSi
		FinSi
	FinSi
FinFuncion


Funcion Ejercicio21()
	//Año bisiesto: Solicita al usuario un año y determina si es un año bisiesto o no. 
	//Un año bisiesto es divisible por 4, pero no por 100, a menos que también sea 
	//divisible por 400.
	
	//Bosquejo
	//Entrada: Año=0?(Leido)
    //Proceso: Si Año mod 4 = 0 y (Año mod 100 <> 0 o Año mod 400 = 0)  El año es bisiesto SiNo el año no es bisiesto
	//Salida: Mostrar si el año es bisiesto o no
	
	//Entrada
	Definir Año Como Entero
	Año=0
	Escribir "Ingrese un año"
	Leer Año
	//Proceso
	Si Año mod 4 = 0 y (Año mod 100 <> 0 o Año mod 400 = 0) Entonces
		Escribir "El año es bisisesto"
	SiNo
		Escribir "El año no es bisisesto"
	FinSi
FinFuncion


Funcion Ejercicio22()
	//Signo zodiacal: Pide al usuario que ingrese su mes y día de nacimiento, luego 
	//determina su signo zodiacal. Puedes usar una serie de declaraciones if para 
	//comparar las fechas ingresadas con las fechas límite de cada signo zodiacal.
	
	//Bosquejo
	//Entrada: mes=0?(Leido) dia=0?(Leido)
	//Proceso: Si (mes = 1) y (dia >= 20) o (mes = 2) y (dia <= 18) Tu signo zodiacal es Acuario.
	//SiNo Si (mes = 2) y (dia >= 19) o (mes = 3) y (dia <= 20)  Tu signo zodiacal es Piscis.
	//SiNo Si (mes = 3) y (dia >= 21) o (mes = 4) y (dia <= 19) Tu signo zodiacal es Aries.
	//SiNo Si (mes = 4) y (dia >= 20) o (mes = 5) y (dia <= 20) Tu signo zodiacal es Tauro.............................
	//Salida: Mostrar el signo zodiacal correspondiente 
	
	Definir mes,dia Como Entero
	mes=0;dia=0
	Escribir "Introduzca un mes en numeros"
	leer mes
	Escribir "Introduzca un día en numeros"
	leer dia
	Si (mes = 1) y (dia >= 20) o (mes = 2) y (dia <= 18) Entonces 
		Escribir "Tu signo zodiacal es Acuario"
	SiNo
		Si (mes = 2) y (dia >= 19) o (mes = 3) y (dia <= 20) Entonces
			Escribir "Tu signo zodiacal es Piscis"
		SiNo
			Si (mes = 3) y (dia >= 21) o (mes = 4) y (dia <= 19) Entonces
				Escribir "Tu signo zodiacal es Aries"
			SiNo 
				Si (mes = 4) y (dia >= 20) o (mes = 5) y (dia <= 20) Entonces
					Escribir"Tu signo zodiacal es Tauro"
				SiNo
					SI (mes = 5) y (dia >= 21) o (mes = 6) y (dia <= 20) Entonces
						Escribir "Tu signo zodiacal es Géminis"
					SiNo
						Si (mes = 6) y (dia >= 21) o (mes = 7) y (dia <= 22) Entonces
							Escribir "Tu signo zodiacal es Cáncer"
						SiNo 
							Si (mes = 7) y (dia >= 23) o (mes = 8) y (dia <= 22) Entonces
								Escribir "Tu signo zodiacal es Leo"
							SiNo
								Si (mes = 8) y (dia >= 23) o (mes = 9) y (dia <= 22) Entonces
									Escribir "Tu signo zodiacal es Virgo"
								SiNo
									Si (mes = 9) y (dia >= 23) o (mes = 10) y (dia <= 22) Entonces
										Escribir "Tu signo zodiacal es Libra"
									SiNo
										Si (mes = 10) y (dia >= 23) o (mes = 11) y (dia <= 21) Entonces
											Escribir "Tu signo zodiacal es Escorpio"
										SiNo
											Si (mes = 11) y (dia >= 22) o (mes = 12) y (dia <= 21) Entonces
												Escribir "Tu signo zodiacal es Sagitario"
											SiNo
												Si (mes = 12) y (dia >= 22) o (mes = 1) y (dia <= 19) Entonces
													Escribir "Tu signo zodiacal es Capricornio"
												SiNo
													Escribir "Error,Ingrese mes y dia validos"
												FinSi
											FinSi
										FinSi
									FinSi
								FinSi	
							FinSi		
						FinSi		
					FinSi
				FinSi
			FinSi
		FinSi
	FinSi

FinFuncion


Funcion Ejercicio23()
	//Día del mes con respecto a la segunda quincena: Solicita al usuario que ingrese 
	//un número de día del mes (por ejemplo, del 1 al 31) y verifica si ese día 
	//pertenece a la primera quincena (días 1-15) o a la segunda quincena (días 16-31).
	
	//Bosquejo:
	//Entrada: Dia=0?(Leido) 
	//Proceso: Si Dia >= 1 Y Dia <= 15 El dia pertenece a la primera quincena SiNo Si Dia >= 16 Y dia <= 31 El Dia pertenece a la segunda quincena
	//SiNo Escribir"Error, ingrese un numero valido"
	//Salida: Mostrar si el dia es de la primera o segunda quincena
	
	//Entrada
	Definir Dia Como Entero
	Dia=0
	Escribir "Ingrese un numero del 1 al 31"
	Leer Dia
	//Proceso
	Si Dia >= 1 Y Dia <= 15 Entonces
		Escribir "El día pertenece a la primera quincena"
	SiNo
		Si Dia >= 16 Y Dia <= 31 Entonces
			Escribir "El día pertenece a la segunda quincena"
		SiNo
			Escribir "Error, ingrese un número valido"
		FinSi
	FinSi
	
FinFuncion


Funcion Ejercicio24()
	// Día de la semana: Pide al usuario que ingrese un número del 1 al 7, donde 1 
	//representa el domingo, 2 el lunes, 3 el martes, y así sucesivamente. Luego, 
	//utiliza una estructura switch para mostrar el nombre del día de la semana 
	//correspondiente al número ingresado
	
	//Bosquejo:
	//Entrada: Num=0?(Leida)
	//Proceso: Si Num=1:Domingo Sino Si Num=2:Lunes Sino Si Num=3:Martes
	//Salida: Mostrar el mensaje del dia
	
	//Entrada
	Definir Num como Entero 
	Num=0
	Escribir "Escriba un Numero del 1 al 7 "
	Leer Num
	//Proceso
	Segun Num Hacer
		1:
			Escribir "1 es Domingo"
		2:
			Escribir "2 es Lunes"
		3:
			Escribir "3 es Martes"
		4:
			Escribir "4 es Miercoles"
		5:
			Escribir "5 es Jueves"
		6:
			Escribir "6 es Viernes"
		7: 
			Escribir "7 es Sabado"
		De Otro Modo:
			Escribir Num," no es un día de la semana"
	Fin Segun
	
	
FinFuncion

Funcion Ejercicio25()
	//Frases iguales: Escribir un programa que ingrese dos frases e indique si son iguales
	
	//Bosquejo
	//Entrada: Frase1=" "?(Leido)  Frase2=" "?(Leido)
	//Proceso: Si Frase1=Frase2 Ambas frases son iguales Sino Las frases no son iguales
	//Salida: Mostrar si las frases son o no iguales
	
	//Entrada
	Definir Frase1,Frase2 Como Caracter
	Frase1=" " ;Frase2=" "
	Escribir "Ingrese Frase 1"
	Leer Frase1
	Escribir "Ingrese Frase 2"
	Leer Frase2
	Si Frase1=Frase2 Entonces
		Escribir "Ambas frases son iguales"
	Sino 
		Escribir "Las frases no son iguales"
		FinSi
FinFuncion


Funcion Ejercicio26()
	//Calculadora de precio con descuento: Crea un programa que permita a un 
	//usuario ingresar el precio de un artículo y un porcentaje de descuento. El 
	//programa debe calcular y mostrar el precio final después del descuento.
	
	//Bosquejo
	//Entrada: Precio=0.0?(Leido) PDesc=0.0?(Leido) Desc=0.0?(Calculo) Pfinal=0.0?(Calculo)
	//Proceso: Desc=Precio*PDesc Pfinal=Precio-Desc
	//Mostrar Precio final despues del descuento
	
	
	//Entrada
	Definir Precio,PDesc,Desc,Pfinal Como Real
	Precio=0.0;PDesc=0.0;Desc=0.0;Pfinal=0.0
	Escribir "Ingrese Precio del artículo"
	Leer Precio
	Escribir "Ingrese su porcentaje de descuento"
	Leer PDesc
	//Proceso
	Desc=Precio*PDesc
	Pfinal=Precio-Desc
	//Salida
	Escribir "Su precio es $",Precio
	Escribir "El descuento es de ",Desc
	Escribir "El precio final es de $",Pfinal
	
FinFuncion


Funcion Ejercicio27()
	//Calculadora de factura con impuestos: Solicita al usuario que ingrese el total 
	//de una factura y el porcentaje de impuestos aplicado. Luego, calcula y muestra 
	//el monto total a pagar, incluyendo los impuestos.
	
	//Bosquejo
	//Entrada: Total=0.0?(Leido) Pimp=0.0?(Leido) Iva=0.0?(Calculo) Pfinal=0.0?(Calculo)
	//Proceso: Iva=Total*Pimp Pfinal=Total+Iva
	//Mostrar pago final incluyendo impuestos
	
	//Entrada
	Definir Total,Pimp,Iva,Pfinal Como Real
	Total=0.0;Pimp=0.0;Iva=0.0;Pfinal=0.0
	Escribir "Ingrese el precio total de su factura"
	Leer Total
	Escribir "Ingrese el porcentaje de impuestos aplicados"
	Leer Pimp
	//Proceso
	Iva=Total*Pimp 
	Pfinal=Total+Iva
	//Salida
	Escribir "El total de su factura es $",Total
	Escribir "Los impuestos aplicados son de $",Iva
	Escribir "El precio de pago final es de $",Pfinal
	
FinFuncion


Funcion Ejercicio28()
	//Calculadora de sueldo con aumento: Pide al usuario que ingrese su salario 
	//actual y el porcentaje de aumento que recibirá. Calcula y muestra el nuevo 
	//salario después del aumento
	
	//Bosquejo
	//Entrada: Sueldo=0.0?(Leido) Paumento=0.0?(Leido) Aumento=0.0?(Calculo) SalarioF=0.0?(Calculo)
	//Proceso: Aumento=Sueldo*Paumento SalarioF=Sueldo+Aumento
	//Mostrar salario,aumento y nuevo salario
	
	
	//Entrada
	Definir Sueldo,Paumento,Aumento,SalarioF Como Real
	Sueldo=0.0;Paumento=0.0;Aumento=0.0;SalarioF=0.0
	Escribir "Ingrese su sueldo actual"
	Leer Sueldo
	Escribir "Ingrese el porcentaje de aumento de sueldo"
	Leer Paumento
	//Proceso
	Aumento=Sueldo*Paumento 
	SalarioF=Sueldo+Aumento
	//Salida
	Escribir "Su salario actual es de $",Sueldo
	Escribir "El aumento de salario es de $",Aumento
	Escribir "Su salario final es de $",SalarioF
FinFuncion


Funcion Ejercicio29()
	//Calculadora de compra con múltiples artículos: 
	//Permite al usuario ingresar el precio y la cantidad de varios artículos que está comprando. 
	//Calcula el total de la compra y aplica un descuento del 10% si el total es mayor a cierta cantidad 
	//(por ejemplo, $100)
	
	//Bosquejo: 
	//Entrada: Precio=0.0?(Leer),Cantidad=0?(Leer),Total=?(Calculo),PDesc=0.0?(Leer),Desc=0.0?(Calculo)
	//Proceso: Total= Precio*Cantidad,
	//Si Total > 100:Desc=Total*PDesc
	//Salida: Mostrar total,descuento,total
	Definir Precio,Total,PDesc,Desc como real
	Definir Cantidad Como Entero
	Precio=0.0;Total=0.0;PDesc=0.10;Desc=0.0;Cantidad=0
	Escribir "Ingrese Precio del producto"
	Leer Precio
	Escribir "Escriba Cantidad del producto"
	Leer Cantidad
	//Proceso
	Total=Precio*Cantidad
	Si Total >100 Entonces 
		Desc=Total*PDesc
	Sino 
		Desc=0
	FinSi
	//Salida
	Escribir"El total es $",Total
	Escribir"Su descuento es de $",Desc
	Escribir"Su pago final es de $",Total-Desc
FinFuncion


Funcion Ejercicio30_31_32()
	//Calculadora de impuestos sobre el salario: Solicita al usuario que ingrese su 
    //salario anual y calcula el impuesto sobre la renta según las siguientes tasas:
	//Hasta $10,000: 5%
	//De $10,001 a $20,000: 10%
    //Más de $20,000: 15%
	
	//Bosquejo
	//Entrada: Salario=0.0?(Leido) Impuesto(Calculado)
	//Proceso Si Salario <= 10000 Impuesto= Salario*0.05 SiNo Si Salario >= 10001 y Salario <= 20000  Impuesto=Salario*0.10
	//SiNo Impuesto=Salario*0.15
	//Salida: Mostrar impuesto sobre la renta de su salario.
	
	//Entrada
	Definir Salario,Impuesto Como Real
	Salario=0.0;Impuesto=0.0 
	Escribir "Ingrese su salario anual"
	Leer Salario
	//Proceso
	Si Salario <= 10000 Entonces
		Impuesto=Salario*0.05
		Escribir "El Impuesto sobre la renta es del 5%"
		Escribir "El Impuesto sobre la renta de su salario es de $", Impuesto
	SiNo
		Si Salario >= 10001 y Salario <= 20000 Entonces
			Impuesto=Salario*0.10
			Escribir "El Impuesto sobre la renta es del 10%"
			Escribir "El Impuesto sobre la renta de su salario es de $", Impuesto
		SiNo
			Impuesto=Salario*0.15
			Escribir "El Impuesto sobre la renta es del 15%"
			Escribir "El Impuesto sobre la renta de su salario es de $", Impuesto
		FinSi
	FinSi
FinFuncion

